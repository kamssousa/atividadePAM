<body>
	<h1>Somas diferentes</h1>
	<p>Basta inspecionar a página</p>
	<script>
		// Função para somar dois números
		function somarNumeros(numero1, numero2) {
			return numero1 + numero2;
		}

		// Exemplos de somas diferentes
		var resultado1 = somarNumeros(5, 3);
		console.log("Resultado da soma 5 + 3: " + resultado1);

		var resultado2 = somarNumeros(10, -2);
		console.log("Resultado da soma 10 + (-2): " + resultado2);
	</script>

